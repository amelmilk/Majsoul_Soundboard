<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.majsoul_soundboard">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ichihime_00"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ichihime_00"
        android:supportsRtl="true"
        android:theme="@style/Theme.Majsoul_Soundboard">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Ichihime_Main"/>
        <activity android:name=".Ichihime_Score"/>
        <activity android:name=".Ichihime_Result"/>
        <activity android:name=".Ichihime_Sign"/>
        <activity android:name=".Miki_Main"/>
        <activity android:name=".Miki_Sign"/>
        <activity android:name=".Miki_Result"/>
        <activity android:name=".Miki_Score"/>


    </application>

</manifest>